{
  "manifest_version": 3,
  "name": "Chrome Live Bookmarks",
  "short_name": "Chrome Live Bookmarks",
  "version": "#version#",
  "default_locale": "en",
  "description": "#description#",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Mohammed Ismail Ansari <team.terminal@gmail.com>",
  "permissions": [
    "bookmarks",
    "history",
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
